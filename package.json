{
  "name": "ag2dExample",
  "version": "1.0.0",
  "description": "A 2d renderer engine with multiple modules to help with common tasks",
  "repository": "https://aarongeorge@github.com/aarongeorge/ag2d.git",
  "author": "Aaron George <aaron.dean.george@gmail.com>",
  "license": "MIT",
  "scripts": {
    "bundle": "browserify ./src/js/main.js -o ./src/js/bundle.js -t [ babelify --presets [ env stage-2 ] ] && uglifyjs ./src/js/bundle.js -o ./src/js/bundle.js",
    "serve": "cd ./src && http-server"
  },
  "dependencies": {
    "ag2d": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "http-server": "^0.10.0",
    "uglify-js": "^3.1.9"
  }
}
